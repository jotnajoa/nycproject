AAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AAC3B;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAClB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;IACb,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACT,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACV;;AAEJ,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;IACtB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACV,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AACf;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACjB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACR,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AACV;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACN,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACjB,CAAC,CAAC,CAAC,CAAC,CAAC;AACT","file":"/Users/soonkpaik/Desktop/NYC Project/code/src/views/BoroughSelection.vue","sourceRoot":"","sourcesContent":["<template>\n\n    <transition name=\"openup\" v-on:after-enter=\"transitionEnd\">\n    \n    <div class='horizontal-container dark-background white-text' v-if='showup' ref='container'>\n        <canvas id='myCanvas' ref='myCanvas'></canvas>\n        <div class='menu-text white-text medium medium-text'>This is Landing Page</div>\n        <year-select class='year-selection'></year-select>\n        <div>{{boroughData}}</div>\n    </div>\n\n    </transition>\n\n</template>\n\n<script>\n\nlet scene, camera, renderer, sphereMesh,raycaster,canvas\nlet frame=0;\n\nimport * as THREE from 'three'\nimport YearSelect from '../components/UI/YearSelect.vue'\nexport default {\n  components: { YearSelect },\n  data(){\n      return{\n          showup:false,\n          sRadius:2,\n          targetCounty:undefined,\n          mouse:{x:undefined,y:undefined},\n          getIntersect:false,\n          cWidth:undefined,\n          cHeight:undefined\n      }\n  },\n  mounted(){\n    this.showup=true;\n    this.$store.dispatch('Landing/loadData',2021)\n\n    const unwatch = this.$watch(\n        \n        () => this.$store.getters['Landing/boroSummary'],\n        boroSummary =>{\n            if(boroSummary){\n                this.boroughData=boroSummary;\n                this.init();\n                this.animate();\n                unwatch()\n            }\n        }\n\n    )\n  },\n  created(){ },\n  methods:{\ntransitionEnd(){\n    const width=this.$refs.container.clientWidth;\n    const height=this.$refs.container.clientHeight;\n    this.cWidth=width;\n    this.cHeight=height;\n    canvas.style.height=`${height}px`\n    canvas.style.width=`${width}px`;\n\ncamera = new THREE.PerspectiveCamera( 75, this.cWidth/this.cHeight, 0.1, 100 );\nrenderer=new THREE.WebGLRenderer({canvas:canvas})\nrenderer.setSize( this.cWidth, this.cHeight );\nrenderer.setPixelRatio(Math.min(window.devicePixelRatio, 2))\ncamera.position.z = 5\n\n\n},\ninit(){\nscene =new THREE.Scene();\ncanvas=this.$refs.myCanvas\nraycaster=new THREE.Raycaster();\nthis.mouse = new THREE.Vector2();\n\n\nthis.cWidth=canvas.clientWidth;\nthis.cHeight=canvas.clientHeight;\n\ncamera = new THREE.PerspectiveCamera( 75, this.cWidth/this.cHeight, 0.1, 100 );\nrenderer=new THREE.WebGLRenderer({canvas:canvas})\nrenderer.setSize( this.cWidth, this.cHeight );\nrenderer.setPixelRatio(Math.min(window.devicePixelRatio, 2))\n\n\n\nlet texture = new THREE.MeshBasicMaterial({color:'red',wireframe:true});\nsphereMesh= new THREE.Mesh(\n    new THREE.SphereGeometry(2,32,32),\n    texture\n)\ncamera.position.z = 5\nscene.add(sphereMesh);\n\n\n\n\n},\n\nanimate(){\n\n    requestAnimationFrame(this.animate);\n    raycaster.setFromCamera(this.mouse,camera);\n/*\n    // const intersect = raycaster.intersectObject(sphereMesh)\n    // const intersectPoints=[];\n    // intersection이 결국 index에 대한 접근을 주니까, \n    // array에서 해당 index\n    // if(intersect.length>0 && this.getIntersect==true){\n\n    // intersect.forEach((d)=>{\n    //     intersectPoints.push(d.index);\n    //     console.log(intersectPoints)\n    // })\n    // console.log(intersect)\n    // }else{\n    //     sphereMesh.material.size=0.05\n    // }\n*/\n\n    frame+=0.0001\n\n    sphereMesh.rotation.x=frame*10\n    sphereMesh.rotation.y=frame*10\n\n    renderer.render(scene, camera);\n}\n\n\n\n  },\n    computed:{\n        boroughData(){\n            return this.$store.getters['Landing/boroSummary']\n        }\n    }\n  \n}\n</script>\n<style scoped>\n.horizontal-container{\n    position:absolute;\n    top:50%;\n    transform:translate(0,-50%);\n    height:70vh;\n    width:100vw;\n    padding:0;\n    margin:0;\n    left:0;\n}\n.openup-enter-active,\n.openup-leave-active {\n  transition: all 0.5s ease;\n}\n\n.openup-enter-from,\n.openup-leave-to {\n    opacity:0;\n    height:0vh;\n    }\n\n#myCanvas{\n    border:solid 1px white;\n    width:100%;\n    height:100%;\n}\n.year-selection{\n    position:absolute;\n    top:2rem;\n    left:0;\n}\n.menu-text{\n    position:absolute;\n    top:0;\n}\n</style>"]}]}