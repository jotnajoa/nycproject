N;;aADO,KAAA,CAAA,QAAA,GAAQ,M;;AACf,GAJF,E,IAAA,E;;AAAA,I,SAAA,C,kBAGS,KAAA,CAAA,Q,KAET,YAAA,CAA6D,OAA7D,EAAA,UAAA,EAA6D,gBAAA,CAAlC,MAAA,CAAA,OAAA,CAAO,CAAP,EAAW,WAAX,EAAkC,CAA7D,EAAiD;AAAA;AAAjD,G,kBAEA,YAAA,CAIE,OAJF,EAIE;AAJK,IAAA,IAAI,EAAC,OAIV;AAJkB,IAAA,EAAE,EAAC,cAIrB;AAJoC,IAAA,IAAI,EAAC,eAIzC;AAHD,IAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,cAAA,IAAA,QAAA,CAAA,cAAA,OAAA,QAAA,YAAF;AAAA,KAAA,CAGL;AAFD,IAAA,KAAK,EAAE,MAAA,CAAA,OAAA,CAAO,CAAP,CAEN;;aADO,KAAA,CAAA,QAAA,GAAQ,M;;AACf,GAJF,E,IAAA,E;;AAAA,I,SAAA,C,kBAGS,KAAA,CAAA,Q,KAET,YAAA,CAA8D,OAA9D,EAAA,UAAA,EAA8D,gBAAA,CAAlC,MAAA,CAAA,OAAA,CAAO,CAAP,EAAW,WAAX,EAAkC,CAA9D,EAAkD;AAAA;AAAlD,G,CAhBJ,E;;AAAA,G,qCAkBG,CAjDP,C","sourcesContent":["\n<template>\n        \n<form class=\"form\">\n    \n    <div class='switch-field'\n    v-if='fiveSelection'>\n\n        <input type=\"radio\" id=\"radio-one\" name=\"switch-one\" :value=\"options[0]\" \n        v-model='selected'/>\n        <label for=\"radio-one\">{{options[0].toUpperCase()}}</label>\n\n        <input type=\"radio\" id=\"radio-two\" name=\"switch-two\" :value=\"options[1]\" \n        v-model='selected'/>\n        <label for=\"radio-two\">{{options[1].toUpperCase()}}</label>\n\n        <input type=\"radio\" id=\"radio-three\" name=\"switch-three\" :value=\"options[2]\" \n        v-model='selected'\n        />\n        <label for=\"radio-three\">{{options[2].toUpperCase()}}</label>\n\n\n        <input type=\"radio\" id=\"radio-four\" name=\"switch-four\" :value=\"options[3]\" \n        v-model='selected'\n        />\n        <label for=\"radio-four\">{{options[3].toUpperCase()}}</label>\n\n\n        <input type=\"radio\" id=\"radio-five\" name=\"switch-five\" :value=\"options[4]\" \n        v-model='selected'\n        />\n        <label for=\"radio-five\">{{options[4].toUpperCase()}}</label>\n\n    </div>\n    <div :class = \"{ 'switch-field' : !smallSelection,\n                     'small-switch-field':smallSelection}\"\n     v-if='twoSelection'>\n\n        <input type=\"radio\" id=\"radio-first\" name=\"switch-first\" \n        @change=\"categoryChange\"\n        :value=\"options[0]\" \n        v-model='selected'\n        />\n        <label for=\"radio-first\">{{options[0].toUpperCase()}}</label>\n\n        <input type=\"radio\" id=\"radio-second\" name=\"switch-second\" \n        @change=\"categoryChange\"\n        :value=\"options[1]\" \n        v-model='selected'\n        />\n        <label for=\"radio-second\">{{options[1].toUpperCase()}}</label>\n    </div>\n</form>\n\n</template>\n\n<script>\nexport default {\n    props:['options','smallSelection'],\n    data(){\n        return{\n            selected:undefined\n            }\n    },\n    emits: ['selectedCategory'],\n    methods:{\n        categoryChange(){\n            this.$emit(\"selectedCategory\", this.selected);\n        }\n    },\n    computed:{\n\n        fiveSelection(){\n            let bool = this.options.length==5? true:false\n            return bool\n            },\n        twoSelection(){\n            let bool = this.options.length==2? true:false;\n            return bool\n            }\n    },\n    mounted(){\n        this.selected=this.options[0]\n    }\n}\n</script>\n\n<style scoped>\n.form{\n    z-index:5\n}\n.switch-field {\n    display: flex;\n    overflow: hidden;\n}\n\n.switch-field input {\n    position: absolute !important;\n    clip: rect(0, 0, 0, 0);\n    height: 1px;\n    width: 1px;\n    border: 0;\n    overflow: hidden;\n}\n\n\n.switch-field label {\n    background-color: #FFFFFF;\n    color: #1A1A1A;\n    font-size: 1rem;\n    line-height: 1;\n    text-align: center;\n    padding: 0.5rem 2rem;\n    /* margin-right: -1px; */\n    border: 1px solid rgba(0, 0, 0, 0.2);\n    box-shadow: inset 0 0.5px 0.5px rgba(0, 0, 0, 0.13);\n    transition: all 0.1s ease-in-out;\n}\n\n\n.switch-field label:hover {\n    cursor: pointer;\n}\n\n.switch-field input:checked + label {\n    background-color: #66c7ff;\n    color:#F1F5FB;\n    box-shadow: none;\n}\n\n.switch-field label:first-of-type {\n    border-radius: 4px 0 0 4px;\n}\n\n.switch-field label:last-of-type {\n    border-radius: 0 4px 4px 0;\n}\n\n\n\n\n\n\n\n.small-switch-field {\n    display: flex;\n    overflow: hidden;\n}\n\n.small-switch-field input {\n    position: absolute !important;\n    clip: rect(0, 0, 0, 0);\n    height: 1px;\n    width: 1px;\n    border: 0;\n    overflow: hidden;\n}\n\n\n.small-switch-field label {\n    background-color: #FFFFFF;\n    color: #1A1A1A;\n    font-size: 0.8rem;\n    line-height: 1;\n    text-align: center;\n    padding: 0.2rem 0.4rem;\n    /* margin-right: -1px; */\n    border: 1px solid rgba(0, 0, 0, 0.2);\n    box-shadow: inset 0 0.5px 0.5px rgba(0, 0, 0, 0.13);\n    transition: all 0.1s ease-in-out;\n}\n\n\n.small-switch-field label:hover {\n    cursor: pointer;\n}\n\n.small-switch-field input:checked + label {\n    background-color: #66c7ff;\n    color:#F1F5FB;\n    box-shadow: none;\n}\n\n.small-switch-field label:first-of-type {\n    border-radius: 4px 0 0 4px;\n}\n\n.small-switch-field label:last-of-type {\n    border-radius: 0 4px 4px 0;\n}\n\n\n\n\n\n</style>"],"sourceRoot":""}]}